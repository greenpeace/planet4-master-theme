{% block html_head_container %}

{% include 'html-header.twig' %}

{% endblock %}

<body class="{{ body_class }}">

	{% if google_tag_value %}
		<!-- Google Tag Manager (noscript) -->
		<noscript><iframe src="https://www.googletagmanager.com/ns.html?id={{ google_tag_value }}"
						  height="0" width="0" style="display:none;visibility:hidden"></iframe>
		</noscript>
		<!-- End Google Tag Manager (noscript) -->
	{% endif %}

	<div class="hero-img-wrap">
		{% include 'navigation-bar.twig' with data_nav_bar %}
	</div>
	{% include 'blocks/header.twig' %}

	{% block content %}
		Sorry, no content
	{% endblock %}

	{% if sidebar %}
		<aside class="layout-sidebar">
			{{sidebar}}
		</aside>
	{% endif %}

	{% block footer %}
		{% include 'footer.twig' %}

		{% block cookies %}
			{% include 'cookies.twig' %}
		{% endblock %}

		{{ function('wp_footer') }}
	{% endblock %}
    <script type="text/javascript" src="https://s7.addthis.com/js/300/addthis_widget.js#pubid=ra-54510c710c3d3d92"></script>
</body>
</html>
